    <div typeof="rdfs:Class" resource="http://schema.org/MortgageLoan">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">MortgageLoan</span>
        <span property="rdfs:comment">A loan in which property or real estate is used as collateral. (A loan securitized against some real estate.)</span>
        <span>Subclass of: <a property="rdfs:subClassOf" href="http://schema.org/LoanOrCredit">LoanOrCredit</a></span>
    </div>

    <div typeof="rdfs:Class" resource="http://schema.org/RepaymentSpecification">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">RepaymentSpecification</span>
        <span property="rdfs:comment">A structured value representing repayment.</span>
        <span>Subclass of: <a property="rdfs:subClassOf" href="http://schema.org/StructuredValue">StructuredValue</a></span>
    </div>

    <div typeof="rdfs:Class" resource="http://schema.org/ExchangeRateSpecification">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">ExchangeRateSpecification</span>
        <span property="rdfs:comment">A structured value representing exchange rate.</span>
        <span>Subclass of: <a property="rdfs:subClassOf" href="http://schema.org/StructuredValue">StructuredValue</a></span>
    </div>

    <div typeof="rdfs:Class" resource="http://schema.org/BrokerageAccount">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">BrokerageAccount</span>
        <span property="rdfs:comment">An account that allows an investor to deposit funds and place investment orders with a licensed broker or brokerage firm.</span>
        <span>Subclass of: <a property="rdfs:subClassOf" href="http://schema.org/InvestmentOrDeposit">InvestmentOrDeposit</a></span>
    </div>

    <div typeof="rdfs:Class" resource="http://schema.org/InvestmentFund">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">InvestmentFund</span>
        <span property="rdfs:comment">A company or fund that gathers capital from a number of investors to create a pool of money that is then re-invested into stocks, bonds and other assets.</span>
        <span>Subclass of: <a property="rdfs:subClassOf" href="http://schema.org/InvestmentOrDeposit">InvestmentOrDeposit</a></span>
    </div>

    <div typeof="rdfs:Class" resource="http://schema.org/MoneyTransfer">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">MoneyTransfer</span>
        <span property="rdfs:comment">The act of transferring money from one place to another place. This may occur electronically or physically.</span>
        <span>Subclass of: <a property="rdfs:subClassOf" href="http://schema.org/TransferAction">TransferAction</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/mobileApp">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">mobileApp</span>
        <span property="rdfs:comment">The location of mobile application used to access the service.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/ServiceChannel">ServiceChannel</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/URL">URL</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/textChat">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">textChat</span>
        <span property="rdfs:comment">The text chat URL to access the service.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/ServiceChannel">ServiceChannel</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/URL">URL</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/email">
        <span class="h" property="rdfs:label">email</span>
        <span property="rdfs:comment">The address to access the service by email.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/ServiceChannel">ServiceChannel</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/bankAccountType">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">bankAccountType</span>
        <span property="rdfs:comment">The type of a bank account.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/BankAccount">BankAccount</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/URL">URL</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Text">Text</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/overdraftLimit">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">overdraftLimit</span>
        <span property="rdfs:comment">An overdraft is an extension of credit from a lending institution when an account reaches zero. An overdraft allows the individual to continue withdrawing money even if the account has no funds in it. Basically the bank allows people to borrow a set amount of money.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/BankAccount">BankAccount</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/minimumInflow">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">minimumInflow</span>
        <span property="rdfs:comment">A minimum amount that has to be paid in every month.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/BankAccount">BankAccount</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/domiciled">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">domiciled</span>
        <span property="rdfs:comment">Whether borrower is a resident of the jurisdiction where the property is located.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/MortgageLoan">MortgageLoan</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Boolean">Boolean</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/loanMortgageMandateAmount">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">loanMortgageMandateAmount</span>
        <span property="rdfs:comment">Amount of mortgage mandate that can be converted into a proper mortgage at a later stage.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/MortgageLoan">MortgageLoan</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/loanType">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">loanType</span>
        <span property="rdfs:comment">The type of a loan or credit.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/LoanOrCredit">LoanOrCredit</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/URL">URL</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Text">Text</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/repaymentForm">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">repaymentForm</span>
        <span property="rdfs:comment">A form of paying back money previously borrowed from a lender. Repayment usually takes the form of periodic payments that normally include part principal plus interest in each payment.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/LoanOrCredit">LoanOrCredit</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/RepaymentSpecification">RepaymentSpecification</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/earlyPrepaymentPenalty">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">earlyPrepaymentPenalty</span>
        <span property="rdfs:comment">The amount to be paid as a penalty in the event of early payment of the loan.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/RepaymentSpecification">RepaymentSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/numberOfPayments">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">numberOfPayments</span>
        <span property="rdfs:comment">The number of payments contractually required at origination to repay the loan. For monthly paying loans this is the number of months from the contractual first payment date to the maturity date.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/RepaymentSpecification">RepaymentSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Number">Number</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/paymentFrequency">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">paymentFrequency</span>
        <span property="rdfs:comment">Frequency of payments due, i.e. number of months between payments. This is defined as a frequency, i.e. the reciprocal of a period of time.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/RepaymentSpecification">RepaymentSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Number">Number</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/paymentAmount">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">paymentAmount</span>
        <span property="rdfs:comment">The amount of money to pay in a single payment.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/RepaymentSpecification">RepaymentSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/downPayment">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">downPayment</span>
        <span property="rdfs:comment">a type of payment made in cash during the onset of the purchase of an expensive good/service. The payment typically represents only a percentage of the full purchase price.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/RepaymentSpecification">RepaymentSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Number">Number</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/gracePeriod">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">gracePeriod</span>
        <span property="rdfs:comment">The period of time after any due date that the borrower has to fulfil its obligations before a default (failure to pay) is deemed to have occurred.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/LoanOrCredit">LoanOrCredit</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Duration">Duration</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/currency">
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/LoanOrCredit">LoanOrCredit</a></span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/ExchangeRateSpecification">ExchangeRateSpecification</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/renegotiable">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">renegotiable</span>
        <span property="rdfs:comment">Whether the terms for payment of interest can be renegotiated during the life of the loan.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/MortgageLoan">LoanOrCredit</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Boolean">Boolean</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/recourseLoan">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">recourseLoan</span>
        <span property="rdfs:comment">The only way you get the money back in the event of default is the security. Recourse is where you still have the opportunity to go back to the borrower for the rest of the money.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/LoanOrCredit">LoanOrCredit</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Boolean">Boolean</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/monthlyMinimumRepaymentAmount">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">monthlyMinimumRepaymentAmount</span>
        <span property="rdfs:comment">The minimum payment is the lowest amount of money that one is required to pay on a credit card statement each month.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/CreditCard">CreditCard</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Number">Number</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/exchangeRate">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">exchangeRate</span>
        <span property="rdfs:comment">The price of a currency in terms of another currency.</span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/ExchangeRateSpecification">ExchangeRateSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/UnitPriceSpecification">UnitPriceSpecification</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/currentExchangeRate">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">currentExchangeRate</span>
        <span property="rdfs:comment">The current price of a currency.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/ExchangeRateSpecification">ExchangeRateSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/UnitPriceSpecification">UnitPriceSpecification</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/exchangeRateSpread">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">exchangeRateSpread</span>
        <span property="rdfs:comment">The difference between the price at which a broker or other intermediary buys and sells foreign currency.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/ExchangeRateSpecification">ExchangeRateSpecification</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Number">Number</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/floorLimit">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">floorLimit</span>
        <span property="rdfs:comment">A floor limit is the amount of money above which credit card transactions must be authorized.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/PaymentCard">PaymentCard</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/MonetaryAmount">MonetaryAmount</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/cashBack">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">cashBack</span>
        <span property="rdfs:comment">A cardholder benefit that pays the cardholder a small percentage of their net expenditures.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/PaymentCard">PaymentCard</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Boolean">Boolean</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Number">Number</a></span>
    </div>
    
    <div typeof="rdf:Property" resource="http://schema.org/contactlessPayment">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">contactlessPayment</span>
        <span property="rdfs:comment">A secure method for consumers to purchase products or services via debit, credit or smartcards by using RFID or NFC technology.</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/PaymentCard">PaymentCard</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Boolean">Boolean</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/beneficiaryBank">
        <link property="http://schema.org/isPartOf" href="http://fibo.schema.org" />
        <span class="h" property="rdfs:label">beneficiaryBank</span>
        <span property="rdfs:comment">A bank or bank’s branch, financial institution or international financial institution operating the beneficiary’s bank account or releasing funds for the beneficiary</span>
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/MoneyTransfer">MoneyTransfer</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/BankOrCreditUnion">BankOrCreditUnion</a></span>
        <span>Range: <a property="http://schema.org/rangeIncludes" href="http://schema.org/Text">Text</a></span>
    </div>

    <div typeof="rdf:Property" resource="http://schema.org/amount">
        <span>Domain: <a property="http://schema.org/domainIncludes" href="http://schema.org/MoneyTransfer">MoneyTransfer</a></span>
    </div>    
</div>
