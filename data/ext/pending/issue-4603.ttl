@prefix : <https://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:owns a rdf:Property ;
    rdfs:label "owns" ;
    rdfs:comment "Things owned by the organization or person." ;
    :domainIncludes :Organization,
        :Person ;
    :inverseOf :owner ;
    :rangeIncludes :Thing .

:owner a rdf:Property ;
    rdfs:label "owner" ;
    rdfs:comment "A person or organization who owns this Thing." ;
    :domainIncludes :Thing ;
    :inverseOf :owns ;
    :rangeIncludes :Organization,
        :Person .
