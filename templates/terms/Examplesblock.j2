<!-- Examples block for term: {{term.id}}-->

{% for ex in examples %}
{% if loop.first %}<b><a  id="examples" title="Link: #examples" href="#examples" class="clickableAnchor"  >Examples</a></b><br/><br/>{% endif %}

<div><a  id="{{ex.getKey()}}" title="Link: #{{ex.getKey()}}" href="#{{ex.getKey()}}" class="clickableAnchor" >Example {{loop.index}}</a></div>
<div class='ds-selector-tabs ds-selector'>
  <div class='selectors'>
    <a data-selects='original_html' class='selected'>Without Markup</a>
    <a data-selects='microdata' class=''>Microdata</a>
    <a data-selects='rdfa' class=''>RDFa</a>
    <a data-selects='jsonld' class=''>JSON-LD</a>
  </div>

<pre class="prettyprint lang-html linenums original_html selected">{{ ex.getHtml() }}</pre>

<pre class="prettyprint lang-html linenums microdata ">{{ ex.getHtml() }}</pre>

<pre class="prettyprint lang-html linenums rdfa ">{{ ex.getRdfa() }}</pre>

<pre class="prettyprint lang-html linenums jsonld ">{{ ex.getJsonld() }}</pre>

</div>
{% endfor %}
