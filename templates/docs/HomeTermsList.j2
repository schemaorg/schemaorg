<!-- HomeTermsList.j2 -->
{% for c in sectionterms %}
    {% set cat = sectionterms[c] %}
    {% if loop.first %}<h2>{{termcount}} Terms defined in the '{{SECTIONNAME}}' section.</h2>{% endif %}
    {% for term in cat["Type"] %}{%if loop.first %}<h4>{{c}} Types ({{cat["Type"]|length}})</h4>{% else %}, {% endif %}{{ sdotermlink(term)|safe }}{% endfor %}
    {% for term in cat["Property"] %}{%if loop.first %}<h4>{{c}} Properties ({{cat["Property"]|length}})</h4>{% else %}, {% endif %}{{ sdotermlink(term)|safe}}{% endfor %}
    {% for term in cat["Enumeration"] %}{%if loop.first %}<h4>{{c}} Enumerations ({{cat["Enumeration"]|length}})</h4>{% else %}, {% endif %}{{ sdotermlink(term)|safe}}{% endfor %}
    {% for term in cat["Enumerationvalue"] %}{%if loop.first %}<h4>{{c}} Enumeration Members ({{cat["Enumerationvalue"]|length}})</h4>{% else %}, {% endif %}{{ sdotermlink(term)|safe}}{% endfor %}
    {% for term in cat["Datatype"] %}{%if loop.first %}<h4>{{c}} Datatypes ({{cat["Datatype"]|length}})</h4>{% else %}, {% endif %}{{ sdotermlink(term)|safe}}{% endfor %}
{% endfor %}    
<!-- HomeTermsList.j2 end-->
