[codespell]
# Ref: https://github.com/codespell-project/codespell#using-a-config-file
skip = .git,*.pdf,*.svg,*.lock,*.css,*.min.*,.codespellrc,releases,l10n,data,*.bundle.js,examples.txt*
check-hidden = true
# some case sensitive hits and full regexes to ignore word compositions etc
ignore-regex = (^\s*"image/\S+": ".*|\b(INH|WEE|self.aks)\b)
ignore-words-list = ect,implementors,coo
